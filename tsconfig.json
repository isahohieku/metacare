{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
        "data/*"
      ]
    },
    "target": "es2015",
    "skipLibCheck": true,
    "module": "commonjs",
    "outDir": "./build",
    "strict": false,
    "moduleResolution": "node",
    "resolveJsonModule": true, 
    "esModuleInterop": true 
  }
}